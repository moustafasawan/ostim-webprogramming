
@{
    Layout = "_Lab2Layout";
    ViewData["Title"] = "Index";
    @model List<Employee>
}

<h2>Employee List</h2>
<table class="table">
    <tr>
        <th>First Name</th>
        <th>Last Name</th>
        <th>Birth Date</th>
        <th>Department</th>
        <th></th>
    </tr>
    @foreach (var employee in Model)
    {
        @Html.DisplayFor(modelItem => employee, "EmployeeListItem")
    }
</table>


