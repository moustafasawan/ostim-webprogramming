@model List<EmployeeSalaryDto>

<h1>Employee Salary Details</h1>

<!-- Display the list of employee salary details -->
<table>
    <thead>
        <tr>
            <th>Id</th>
            <th>Full Name</th>
            <th>Company Name</th>
            <th>Net Salary</th>
            <th>Gross Salary</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var employee in Model)
        {
            <tr>
                <td>@employee.Id</td>
                <td>@employee.FullName</td>
                <td>@employee.CompanyName</td>
                <td>@employee.NetSalary</td>
                <td>@employee.GrossSalary</td>
            </tr>
        }
    </tbody>
</table>